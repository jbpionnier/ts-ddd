{
  "name": "@jbpionnier/ddd",
  "version": "1.1.0",
  "description": "Domain-Driven Design TypeScript",
  "author": "Jean-Baptiste Pionnier",
  "license": "MIT",
  "repository": "jbpionnier/ts-ddd",
  "keywords": [
    "DDD",
    "CQRS",
    "ES",
    "event sourcing",
    "command",
    "query",
    "event bus",
    "query bus",
    "command bus",
    "base",
    "aggregate",
    "domain",
    "typescript"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write 'src/**/*.ts'",
    "build": "tsc",
    "prepare": "npm run lint && npm run clean && npm run build"
  },
  "dependencies": {
    "tiny-types": "^1.16.1"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@jbpionnier/eslint-config-node": "^1.7.9",
    "@types/node": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  }
}
